# Railway Environment Variables Template
# Copy these to your Railway project's environment variables

# Django Settings
DEBUG=False
SECRET_KEY=your-secret-key-here-generate-new-one
ALLOWED_HOSTS=.railway.app,.up.railway.app,localhost,127.0.0.1

# Database (Railway will provide this automatically when you add PostgreSQL)
# DATABASE_URL=postgresql://user:password@host:port/database

# CORS Settings (Add your frontend URL when deployed)
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# Railway automatically provides these:
# PORT=8000
# RAILWAY_ENVIRONMENT=production
# RAILWAY_PROJECT_ID=your-project-id
# RAILWAY_SERVICE_ID=your-service-id
